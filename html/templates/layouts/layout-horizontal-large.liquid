    {{ "plugins/smartmenus/addons/bootstrap/jquery.smartmenus.bootstrap.js" | asset_url | script_tag }}
 -->
    <!-- Load page level scripts-->
    {% block scripts %}{% endblock %}
    <!-- End loading page level scripts-->

    </body>
</html>