{% include header %}
    <!-- The following CSS are included as plugins and can be removed if unused-->
    {% block styles %}{% endblock %}
    </head>

    <body class="horizontal-nav animated-content infobar-overlay">
        {% include analytics %}
        <div id="wrapper">
            {% include topnav-horizontal %}

            <nav class="navbar navbar-default" role="navigation" id="headernav">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#horizontal-navbar">
                        <i class="fa fa-bars"></i>
                    </button>
                </div>
                <div class="collapse navbar-collapse large-icons-nav" id="horizontal-navbar">
                    <ul class="nav navbar-nav">
                        <li class="active"><a href="index.html"><i class="fa fa-home"></i> <span>Dashboard</span></a></li>
                        <li class="dropdown right">
                            <a href="#" class="dropdown-toggle"><i class="fa fa-envelope"></i> <span>Mail <i class="ml-xs fa fa-angle-down"></i></span></a>
                            <ul class="dropdown-menu">
                                <li><a href="#">Menu Item 1</a></li>
                                <li><a href="#">Menu Item 2</a></li>
                                <li><a href="#">Menu Item 3</a></li>
                                <li><a href="#">Menu Item 4</a></li>
                            </ul>
                        </li>
                        <li><a href="index.html"><i class="fa fa-columns"></i> <span>Layout</span></a></li>
                        <li><a href="index.html"><i class="fa fa-user"></i> <span>Members</span></a></li>
                        <li><a href="index.html"><i class="fa fa-pencil"></i> <span>Components</span></a></li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle"><i class="fa fa-cog"></i> <span>Settings <i class="ml-xs fa fa-angle-down"></i></span></a>
                            <ul class="dropdown-menu">
                                <li><a href="#">Menu Item 1</a></li>
                                <li><a href="#">Menu Item 2</a></li>
                                <li><a href="#">Menu Item 3</a></li>
                                <li><a href="#">Menu Item 4</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>

            <div class="static-content-wrapper">
                <div class="static-content">
                    <div class="page-content">
                        <ol class="breadcrumb mb-md">
                            {% block breadcrumb %}<li><a href="index.html">Home</a></li>{% endblock %}
                        </ol>
                        <div class="container-fluid">
                            {% block content %}{% endblock %}
                        </div> <!-- .container-fluid -->
                    </div> <!-- #page-content -->
                </div>
            </div>

        </div>

        <div class="infobar-wrapper scroll-pane">
            {% include rightbar %}
        </div>

        {% include footer %}



    {% include demo-switcher %}

    {% include scripts %}

    <script type="text/javascript">
        $(function () {
            $('#horizontal-navbar ul.nav.navbar-nav li.dropdown').hover(function () {
                $(this).addClass('open');
            }, function () {
                $(this).removeClass('open');
            })
        })
    </script>
