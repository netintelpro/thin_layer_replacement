{% extends 'layout' %}
{% block title %}Form Components{% endblock %}

{% block breadcrumb %}
<li><a href="index.html">Home</a></li>
<li><a href="#">Advanced Forms</a></li>
<li class="active"><a href="ui-forms.html">Form Components</a></li>
{% endblock %}

{% block styles %}
{{ "plugins/form-select2/select2.css" | asset_url | stylesheet_tag }}                        <!-- Select2 -->
{{ "plugins/form-fseditor/fseditor.css" | asset_url | stylesheet_tag }}                      <!-- FullScreen Editor -->
{{ "plugins/bootstrap-tokenfield/css/bootstrap-tokenfield.css" | asset_url | stylesheet_tag }}   <!-- Tokenfield -->

{{ "plugins/bootstrap-touchspin/dist/jquery.bootstrap-touchspin.min.css" | asset_url | stylesheet_tag }} <!-- Touchspin -->

{{ "plugins/card/lib/css/card.css" | asset_url | stylesheet_tag }} 						 <!-- Card -->

{% endblock %}

{% block scripts %}
{{ "plugins/quicksearch/jquery.quicksearch.min.js" | asset_url | script_tag }}           			<!-- Quicksearch to go with Multisearch Plugin -->
{{ "plugins/form-typeahead/typeahead.bundle.min.js" | asset_url | script_tag }}                 	<!-- Typeahead for Autocomplete -->
{{ "plugins/form-select2/select2.min.js" | asset_url | script_tag }}                     			<!-- Advanced Select Boxes -->
{{ "plugins/form-autosize/jquery.autosize-min.js" | asset_url | script_tag }}            			<!-- Autogrow Text Area -->
{{ "plugins/form-colorpicker/js/bootstrap-colorpicker.min.js" | asset_url | script_tag }} 			<!-- Color Picker -->

{{ "plugins/bootstrap-touchspin/dist/jquery.bootstrap-touchspin.js" | asset_url | script_tag }}      <!-- Touchspin -->

{{ "plugins/form-fseditor/jquery.fseditor-min.js" | asset_url | script_tag }}            			<!-- Fullscreen Editor -->
{{ "plugins/form-jasnyupload/fileinput.min.js" | asset_url | script_tag }}               			<!-- File Input -->
{{ "plugins/bootstrap-tokenfield/bootstrap-tokenfield.min.js" | asset_url | script_tag }}     		<!-- Tokenfield -->

{{ "plugins/card/lib/js/card.js" | asset_url | script_tag }} 										<!-- Card -->

{{ "plugins/jquery-chained/jquery.chained.min.js" | asset_url | script_tag }} 						<!-- Chained Select Boxes -->

{{ "plugins/jquery-mousewheel/jquery.mousewheel.min.js" | asset_url | script_tag }} <!-- MouseWheel Support -->

{{ "demo/demo-formcomponents.js" | asset_url | script_tag }}
{% endblock %}

