                        <%= case_study.country %>

                        
                        <%= link_to 'Show', case_study, class: "btn btn-primary" %>
                        <%= link_to 'Destroy', case_study, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-warning" %>

        
